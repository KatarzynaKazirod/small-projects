    "import math\n",
    "\n",
    "def EulersNumber(precision):\n",
    "    fsum = 1\n",
    "    for i in range(1, precision + 1):\n",
    "        factorials = 1/math.factorial(i)\n",
    "        fsum = fsum + factorials\n",
    "    return fsum\n",
    "\n",
    "print(EulersNumber(100))"
   ]
  }
